{
  "servers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "aws-nx-mcp": {
      "command": "npx",
      "args": ["-y", "-p", "@aws/nx-plugin", "aws-nx-mcp"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    }
  }
}